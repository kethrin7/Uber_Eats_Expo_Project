{
  "expo": {
    "name": "my-uber-eats",
    "slug": "my-uber-eats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5E5E5"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "fonts": [
        {
          "asset": "assets/fonts/UberMoveText-Bold.woff2"
        },
        {
          "asset": "assets/fonts/UberMoveMedium.otf"
        }
      ]
    }
  }
}
